{
    "version": 2,
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/style\\.css",
        "headers": {
          "Content-Type": "text/css"
        }
      },
      {
        "src": "/script\\.js",
        "headers": {
          "Content-Type": "application/javascript"
        }
      },
      {
        "src": "/(.*)",
        "dest": "index.js"
      }
    ]
  }